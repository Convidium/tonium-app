#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Admin/Desktop/tonium-app/node_modules/.pnpm/svelte-check@4.2.2_picomatc_b6f3dd49fd7d47733f2f8212ff9bc4a7/node_modules/svelte-check/bin/node_modules:/mnt/c/Users/Admin/Desktop/tonium-app/node_modules/.pnpm/svelte-check@4.2.2_picomatc_b6f3dd49fd7d47733f2f8212ff9bc4a7/node_modules/svelte-check/node_modules:/mnt/c/Users/Admin/Desktop/tonium-app/node_modules/.pnpm/svelte-check@4.2.2_picomatc_b6f3dd49fd7d47733f2f8212ff9bc4a7/node_modules:/mnt/c/Users/Admin/Desktop/tonium-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Admin/Desktop/tonium-app/node_modules/.pnpm/svelte-check@4.2.2_picomatc_b6f3dd49fd7d47733f2f8212ff9bc4a7/node_modules/svelte-check/bin/node_modules:/mnt/c/Users/Admin/Desktop/tonium-app/node_modules/.pnpm/svelte-check@4.2.2_picomatc_b6f3dd49fd7d47733f2f8212ff9bc4a7/node_modules/svelte-check/node_modules:/mnt/c/Users/Admin/Desktop/tonium-app/node_modules/.pnpm/svelte-check@4.2.2_picomatc_b6f3dd49fd7d47733f2f8212ff9bc4a7/node_modules:/mnt/c/Users/Admin/Desktop/tonium-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
